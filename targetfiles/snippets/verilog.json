{
	"module with parameters": {
		"prefix": ["paramod", "module #"],
		"body": [
			"module ${1:moduleName} #(",
			"\t${2:parameters}",
			") (",
			"\t${3:ports}",
			");",
			"\t$0",
			"endmodule"
		],
		"description": "Insert a module with parameter"
	},
	"module without parameters": {
		"prefix": "module",
		"body": [
			"module ${1:moduleName} (",
			"\t${2:ports}",
			");",
			"\t$0",
			"endmodule"
		],
		"description": "Insert a module without parameter"
	},
	"always": {
		"prefix": ["al", "always"],
		"body": [
			"always @($1) begin",
			"\t$2",
			"end"
		],
		"description": "Insert an always block"
	},
	"posedge": {
		"prefix": "posedge",
